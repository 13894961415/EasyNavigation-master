<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.next.easynavigationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/e_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.next.easynavigationdemo.ui.DemoListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.next.easynavigationdemo.ui.NormalActivity" />
        <activity android:name="com.next.easynavigationdemo.ui.WeiboActivity" />
        <activity android:name="com.next.easynavigationdemo.ui.AddAsFragmentActivity" />
        <activity android:name="com.next.easynavigationdemo.ui.AddViewActivity" />
    </application>

</manifest>